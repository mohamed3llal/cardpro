{
  "name": "card-management-api",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "start": "node --dns-result-order=ipv4first dist/server.js",
    "dev": "set NODE_OPTIONS=--dns-result-order=ipv4first && nodemon --watch src --ext ts,json --exec ts-node -r tsconfig-paths/register src/server.ts",
    "dev:watch": "concurrently \"tsc --watch\" \"nodemon dist/server.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "seed": "set NODE_OPTIONS=--dns-result-order=ipv4first && ts-node -r tsconfig-paths/register src/scripts/seedDomains.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dns": "^0.2.2",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.2.1",
    "google-auth-library": "^10.4.0",
    "helmet": "^7.2.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.3",
    "morgan": "^1.10.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.19",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
